{
  "name": "gui",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run parse-stub-examples && vite --host 0.0.0.0 --port 10001",
    "build": "npm run parse-stub-examples && npm run lint && npm run typecheck && vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "parse-stub-examples": "node scripts/example-parsing/parse-examples.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fontsource/roboto-mono": "^4.5.8",
    "@microsoft/signalr": "^6.0.10",
    "bootstrap": "^5.2.2",
    "bootstrap-icons": "^1.9.1",
    "codemirror": "^5.65.3",
    "dayjs": "^1.11.6",
    "highlight.js": "^11.6.0",
    "js-yaml": "^4.1.0",
    "marked": "^4.1.1",
    "pinia": "^2.0.23",
    "register-service-worker": "^1.7.2",
    "sprintf-js": "^1.1.2",
    "toastr": "^2.1.4",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vue3-slide-up-down": "^1.2.5",
    "xml-formatter": "^2.6.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/bootstrap": "^5.2.5",
    "@types/codemirror": "^5.60.5",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^20.0.0",
    "@types/marked": "^4.0.7",
    "@types/node": "^18.11.3",
    "@types/sprintf-js": "^1.1.2",
    "@types/toastr": "^2.1.40",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.6.0",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.1",
    "mustache": "^4.2.0",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
  }
}
