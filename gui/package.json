{
  "name": "gui",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run parse-stub-examples && vite --host 0.0.0.0 --port 10001",
    "build": "npm run parse-stub-examples && npm run lint && npm run typecheck && npm run test:unit:run && vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:unit:run": "vitest --environment jsdom --run",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "parse-stub-examples": "node scripts/example-parsing/parse-examples.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fontsource/roboto-mono": "^4.5.10",
    "@microsoft/signalr": "^7.0.5",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.5",
    "codemirror": "^5.65.13",
    "dayjs": "^1.11.7",
    "highlight.js": "^11.8.0",
    "js-yaml": "^4.1.0",
    "marked": "^5.0.1",
    "pinia": "^2.0.36",
    "sprintf-js": "^1.1.2",
    "toastr": "^2.1.4",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue3-slide-up-down": "^2.0.1",
    "xml-formatter": "^3.3.2"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/bootstrap": "^5.2.6",
    "@types/codemirror": "^5.60.7",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^21.1.1",
    "@types/marked": "^4.3.0",
    "@types/node": "^20.1.0",
    "@types/sprintf-js": "^1.1.2",
    "@types/toastr": "^2.1.40",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "eslint": "^8.40.0",
    "eslint-plugin-vue": "^9.11.1",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.0.0",
    "mustache": "^4.2.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-singlefile": "^0.13.5",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  }
}
