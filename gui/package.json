{
  "name": "gui",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run parse-stub-examples && vite --host 0.0.0.0 --port 10001",
    "build": "npm run parse-stub-examples && npm run lint && npm run typecheck && npm run test:unit:run && vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:unit:run": "vitest --environment jsdom --run",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "parse-stub-examples": "node scripts/example-parsing/parse-examples.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.5",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.2.0",
    "@codemirror/lang-xml": "^6.0.2",
    "@codemirror/language": "^6.9.0",
    "@codemirror/legacy-modes": "^6.3.3",
    "@codemirror/view": "^6.16.0",
    "@fontsource/roboto-mono": "^5.0.8",
    "@lezer/highlight": "^1.1.6",
    "@microsoft/signalr": "^7.0.10",
    "bootstrap": "^5.3.1",
    "bootstrap-icons": "^1.10.5",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.9",
    "highlight.js": "^11.8.0",
    "js-yaml": "^4.1.0",
    "marked": "^7.0.4",
    "mime-types": "^2.1.35",
    "pinia": "^2.1.6",
    "sprintf-js": "^1.1.2",
    "toastr": "^2.1.4",
    "vue": "^3.3.4",
    "vue-pdf-embed": "^1.1.6",
    "vue-router": "^4.2.4",
    "vue3-slide-up-down": "^2.0.1",
    "xml-formatter": "^3.5.0"
  },
  "devDependencies": {
    "@babel/types": "^7.22.10",
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/bootstrap": "^5.2.6",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^21.1.2",
    "@types/marked": "^5.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.5.3",
    "@types/sprintf-js": "^1.1.2",
    "@types/toastr": "^2.1.40",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "mustache": "^4.2.0",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-singlefile": "^0.13.5",
    "vitest": "^0.34.2",
    "vue-tsc": "^1.8.8"
  }
}
