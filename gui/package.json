{
  "name": "gui",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run parse-stub-examples && vite --host 0.0.0.0 --port 10001",
    "build": "npm run parse-stub-examples && npm run lint && npm run typecheck && npm run test:unit:run && vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:unit:run": "vitest --environment jsdom --run",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "parse-stub-examples": "node scripts/example-parsing/parse-examples.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.6",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.2.2",
    "@codemirror/lang-xml": "^6.0.2",
    "@codemirror/language": "^6.9.2",
    "@codemirror/legacy-modes": "^6.3.3",
    "@codemirror/view": "^6.22.0",
    "@fontsource/roboto-mono": "^5.0.15",
    "@lezer/highlight": "^1.2.0",
    "@microsoft/signalr": "^8.0.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.10",
    "highlight.js": "^11.9.0",
    "js-yaml": "^4.1.0",
    "marked": "^10.0.0",
    "mime-types": "^2.1.35",
    "pinia": "^2.1.7",
    "sprintf-js": "^1.1.3",
    "toastr": "^2.1.4",
    "vue": "^3.3.9",
    "vue-pdf-embed": "^1.2.1",
    "vue-router": "^4.2.5",
    "vue3-slide-up-down": "^2.0.1",
    "xml-formatter": "^3.6.0"
  },
  "devDependencies": {
    "@babel/types": "^7.23.4",
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.6.0",
    "@types/bootstrap": "^5.2.10",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.6",
    "@types/marked": "^5.0.2",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.0",
    "@types/sprintf-js": "^1.1.4",
    "@types/toastr": "^2.1.43",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "mustache": "^4.2.0",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "typescript": "~5.3.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  }
}
