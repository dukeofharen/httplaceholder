<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>HttPlaceholder.Client</AssemblyName>
    <PackageId>HttPlaceholder.Client</PackageId>
    <OutputType>Library</OutputType>
    <Authors>Ducode</Authors>
    <Company>Ducode</Company>
    <RepositoryUrl>https://github.com/dukeofharen/httplaceholder</RepositoryUrl>
    <Description>A very flexible cross platform HTTP stub application.</Description>
    <AssemblyTitle>OpenAPI Library</AssemblyTitle>
    <Copyright>Ducode</Copyright>
    <RootNamespace>HttPlaceholder.Client</RootNamespace>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\HttPlaceholder.Client.xml</DocumentationFile>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>Minor update</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JsonSubTypes" Version="1.5.2" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="RestSharp" Version="106.10.1" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.5.0" />
  </ItemGroup>

</Project>
